<p id="notice"><%= notice %></p>

<h1>Listing Reports</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Wann</th>
      <th>Ort</th>
      <th>Beschr ort</th>
      <th>Ereignis</th>
      <th>Kategorie</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.user_id %></td>
        <td><%= report.wann %></td>
        <td><%= report.ort %></td>
        <td><%= report.beschr_ort %></td>
        <td><%= report.ereignis %></td>
        <td><%= report.kategorie %></td>
        <td><%= link_to 'Show', report %></td>
        <td><%= link_to 'Edit', edit_report_path(report) %></td>
        <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

