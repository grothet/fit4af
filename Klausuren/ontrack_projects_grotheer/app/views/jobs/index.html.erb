<p id="notice"><%= notice %></p>

<h1>Listing Jobs</h1>
<h3>Registrierungen f√ºr <%= @Project.title %></h3>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.title %></td>
        <td><%= job.description %></td>
        <td><%= job.project_id %></td>
        <td><%= link_to 'Job', project_job_path(@project.id, job.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

